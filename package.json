    "@vitest/coverage-v8": "^0.34.4",
    "eslint": "^8.49.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.7.1",
    "typescript": "^5.2.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.33.0"
  }
